{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "VisualEditor0",
            "Effect": "Allow",
            "Action": [
                "apigateway:PUT",
                "apigateway:POST",
                "apigateway:GET"
            ],
            "Resource": [
                "arn:aws:apigateway:us-west-2::/restapis",
                "arn:aws:apigateway:us-west-2::/tags/*"
            ]
        },
        {
            "Sid": "VisualEditor1",
            "Effect": "Allow",
            "Action": [
                "apigateway:DELETE",
                "apigateway:GET"
            ],
            "Resource": "arn:aws:apigateway:us-west-2::/restapis/*",
            "Condition": {
                "StringEquals": {
                    "aws:ResourceTag/CreatedBy": "SLF::APIGateway::Private"
                }
            }
        }
    ]
}