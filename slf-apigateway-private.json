{
    "typeName": "SLF::APIGateway::Private",
    "description": "Create private-only API Gateway objects",
    "properties": {
        "Name": {
            "description": "Name of API Gateway",
            "type": "string",
            "minLength": 3,
            "maxLength": 250
        },
        "Description": {
            "description": "Description of API Gateway",
            "type": "string",
            "minLength": 3,
            "maxLength": 250
        },
        "restApiId": {
            "description": "Unique ID for REST API",
            "type": "string"
        },
        "RootResourceId": {
            "description": "ID for the root resource (i.e. /)",
            "type": "string"
        }
    },
    "additionalProperties": false,
    "required": [
        "Name",
        "Description"
    ],
    "readOnlyProperties": [
        "/properties/RootResourceId"
    ],
    "createOnlyProperties": [
        "/properties/Name",
        "/properties/Description",
        "/properties/RootResourceId"
    ],
    "primaryIdentifier": [
        "/properties/restApiId"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "apigateway:POST",
                "apigateway:GET",
                "apigateway:PUT"
            ]
        },
        "read": {
            "permissions": [
                "apigateway:GET"
            ]
        },
        "delete": {
            "permissions": [
                "apigateway:DELETE"
            ]
        }
    }
}
